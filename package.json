{
  "name": "net-core-mvc-scaffolding",
  "displayName": ".Net Core MVC Controller and View Scaffolding",
  "description": "Visual Studio Scaffolding for .Net Core MVC Controllers and Views in VSCode",
  "version": "0.0.5",
  "publisher": "LeviBickel",
  "repository": {
    "type": "git",
    "url": "https://github.com/LeviBickel/net-core-scaffolding"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "capabilities": {
    "untrustedWorkspaces": {
        "supported": true,
        "description": "The extension needs access to files for scaffolding."
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.scaffoldMVC",
        "title": "Scaffold MVC Controller"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.scaffoldMVC",
          "when": "resourceExtname == '.cs'",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@vscode/vsce": "^3.2.1",
    "glob": "^11.0.0",
    "inflight": "^1.0.6",
    "vsce": "^2.15.0"
  }
}
